	var uid = 0;

	module.exports = function (fn) {
	  if (!fn.hasOwnProperty('toString')) {
	    (function () {
	      var mark = uid += 1;
	      fn.toString = function () {
	        return 'microcosm_tagged_' + mark;
	      };
	    })();
	  }
	};
