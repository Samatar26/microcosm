/* 5 */
/***/ function(module, exports, __webpack_require__) {

	/**
	 * Store
	 * Used to provide default values for a store configuration
	 */

	var skip = function skip(i) {
	  return i;
	};

	function Store(config, id) {
	  // Fold configuration over self
	  for (var i in config) {
	    this[i] = config[i];
	  }

	  this.toString = function () {
	    return id;
	  };
	}

	Store.prototype = {
	  getInitialState: skip,
	  serialize: skip,
	  deserialize: skip,

	  register: function register() {
	    return this;
	  },

	  send: function send(state, action, params) {
	    var task = this.register()[action];
	    return task ? task(state, params) : state;
	  }
	};

	module.exports = Store;
	//# sourceMappingURL=data:application/json;base64,

