/***/ function(module, exports, __webpack_require__) {

	/**
	 * A tree data structure that spawns branches pointing to
	 * various locations within itself.
	 *
	 * @param {Object} state - The initial state of the instance
	 */

	var assoc = __webpack_require__(12);
	var dissoc = __webpack_require__(13);
	var getIn = __webpack_require__(1);
	var Diode = __webpack_require__(10);

	function Foliage(state) {
	  Diode(this);

	  this._path = [];
	  this._root = this;

	  this.commit(state);
	}

	Foliage.prototype = {

	  getPath: function getPath(key) {
	    return this._path.concat(key).filter(function (i) {
	      return i !== void 0;
	    });
	  },

	  getRoot: function getRoot() {
	    return this._root;
	  },

	  commit: function commit(state) {
	    this.getRoot()._state = state;
	    this.volley(state);
	  },

	  clear: function clear() {
	    this.commit();
	  },

	  get: function get(key, fallback) {
	    return getIn(this._state, this.getPath(key), fallback);
	  },

	  set: function set(key, value) {
	    if (arguments.length === 1) {
	      value = arguments[0];
	      key = void 0;
	    }

	    this.commit(assoc(this._state, this.getPath(key), value));
	  },

	  update: function update(key, obj) {
	    if (arguments.length === 1) {
	      obj = arguments[0];
	      key = void 0;
	    }

	    for (var prop in obj) {
	      this.set([key, prop], obj[prop]);
	    }
	  },

	  remove: function remove(key) {
	    this.commit(dissoc(this._state, this.getPath(key)));
	  },

	  refine: function refine(key) {
	    return Object.create(this, {
	      _path: { value: this.getPath(key) }
	    });
	  },

	  keys: function keys() {
	    return Object.keys(this.valueOf() || {});
	  },

	  values: function values() {
	    // An anonymous function is used here instead of
	    // calling `this.get` directly because we have no
	    // fallback value.
	    return this.keys().map(function (key) {
	      return this.get(key);
	    }, this);
	  },

	  valueOf: function valueOf() {
	    return getIn(this._state, this.getPath());
	  },

	  toJSON: function toJSON() {
	    return this.valueOf();
	  },

	  is: function is(branch) {
	    return branch.valueOf() == this.valueOf();
	  },

	  find: function find(fn, scope) {
	    return this.filter(fn, scope)[0];
	  },

	  includes: function includes(value) {
	    return this.indexOf(value) > -1;
	  },

	  first: function first() {
	    return this.values().shift();
	  },

	  last: function last() {
	    return this.values().pop();
	  },

	  size: function size() {
	    return this.values().length;
	  }

	};

	// Add collection methods
	var methods = ['sort', 'map', 'reduce', 'filter', 'forEach', 'some', 'every', 'join', 'indexOf'];

	methods.forEach(function (name) {
	  Foliage.prototype[name] = function () {
	    var _values;

	    return (_values = this.values())[name].apply(_values, arguments);
	  };
	});

	module.exports = Foliage;
	//# sourceMappingURL=data:application/json;base64,

