/***/ function(module, exports, __webpack_require__) {

	/**
	 * Install
	 * A simple FIFO queue for installing plugins
	 */

	var install = function install(_ref, app, callback) {
	  var plugin = _ref[0];

	  var tail = _ref.slice(1);

	  if (!plugin) return callback();

	  var driver = plugin[0];
	  var options = plugin[1];

	  driver.register(app, options, function (err) {
	    if (err) throw err;
	    install(tail, app, callback);
	  });
	};

	module.exports = install;
	//# sourceMappingURL=data:application/json;base64,

