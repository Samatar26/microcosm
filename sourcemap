	    var _this = this;

	    return remap(this.stores, function (store) {
	      return store.serialize(_this.get(store));
	    });
	  };

	  Microcosm.prototype.deserialize = function deserialize() {
	    var _this2 = this;

	    var data = arguments[0] === undefined ? {} : arguments[0];

	    return remap(data, function (state, key) {
	      return _this2.stores[key].deserialize(state);
	    });
	  };

	  Microcosm.prototype.toJSON = function toJSON() {
	    return this.serialize();
	  };

	  Microcosm.prototype.toObject = function toObject() {
	    return this.valueOf();
	  };

	  Microcosm.prototype.start = function start() {
	    var _this3 = this;

	    var callbacks = arguments;

	    this.reset();

	    // Queue plugins and then notify that installation has finished
	    install(this.plugins, this, function () {
	      return run(callbacks, [], _this3);
	    }, 'start');

	    return this;
	  };

	  Microcosm.prototype.push = function push(action, params) {
	    for (var _len = arguments.length, callbacks = Array(_len > 2 ? _len - 2 : 0), _key = 2; _key < _len; _key++) {
	      callbacks[_key - 2] = arguments[_key];
	    }

	    var _this4 = this;

	    return Signal(action, params, function (error, result) {
	      if (!error) {
	        _this4.dispatch(action, result);
	      }

	      run(callbacks, [error, result], _this4, 'push');
	    });
	  };

	  Microcosm.prototype.dispatch = function dispatch(action, params) {
	    tag(action);

	    for (var key in this.stores) {
	      var state = this.get(key);
	      var store = this.stores[key];

	      this.set(key, store.send(state, action, params));
	      this.volley();
	    }

	    return params;
	  };

	  return Microcosm;
