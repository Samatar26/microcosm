/***/ function(module, exports, __webpack_require__) {

	/**
	 * Copy
	 * Shallow copy an object.
	 *
	 * @param {Object} obj - The target object
	 */

	module.exports = function copy(obj) {
	  // Arrays get out cheap
	  if (Array.isArray(obj)) return obj.slice();

	  var clone = {};

	  for (var key in obj) {
	    clone[key] = obj[key];
	  }

	  return clone;
	};
