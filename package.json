{
  "name": "microcosm",
  "version": "9.20.0",
  "private": true,
  "description": "A variant of Facebook's Flux with centralized, isolated state",
  "main": "src/Microcosm.js",
  "jsnext:main": "src/Microcosm.js",
  "scripts": {
    "prepublish": "make build",
    "start": "make example",
    "test": "make test",
    "test:watch": "make test-watch",
    "test:fast": "make test-fast",
    "release": "make release"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vigetlabs/microcosm.git"
  },
  "keywords": [
    "react",
    "flux"
  ],
  "license": "MIT",
  "dependencies": {
    "diode": "6.2.3",
    "hoist-non-react-statics": "1.0.3"
  },
  "babel": {
    "stage": 1,
    "loose": "all",
    "sourceMaps": "inline",
    "env": {
      "production": {
        "optional": [
          "utility.inlineEnvironmentVariables"
        ]
      }
    }
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "console.table": "0.4.0",
    "elizabot": "0.0.2",
    "history": "1.13.1",
    "istanbul": "~0.4.2",
    "istanbul-instrumenter-loader": "0.1.3",
    "karma": "~0.13.15",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-mocha": "~0.2.0",
    "karma-mocha-reporter": "~1.1.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "microcosm-debugger": "0.0.0",
    "mocha": "~2.3.3",
    "mustache": "2.2.1",
    "mustache-loader": "0.3.1",
    "paths-js": "~0.3.5",
    "promise": "~7.1.1",
    "react": "~0.14.2",
    "react-addons-test-utils": "~0.14.3",
    "react-dom": "~0.14.2",
    "react-router": "1.0.2",
    "sprout-data": "0.2.3",
    "static-site-generator-webpack-plugin": "2.0.1",
    "uid": "0.0.2",
    "webpack": "~1.12.11",
    "webpack-dev-server": "~1.14.1"
  }
}
